generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url = env("DATABASE_URL")
  relationMode = "prisma"
}

model user{
  id          Int     @id @default(autoincrement())
  username    String  @db.VarChar(250)
  email       String  @db.VarChar(250)
  phonenumber String  @db.VarChar(250)
}